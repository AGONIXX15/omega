prefix=${pcfiledir}/../..
includedir=${prefix}/include
libdir=${prefix}/lib

Name: Pango Cairo
Description: Cairo rendering support for Pango
Version: 1.56.1

Libs: "-L${libdir}" -lpangocairo-1.0 -lm
Requires: pango, pangoft2, glib-2.0 >=  2.80, gobject-2.0 >=  2.80, gio-2.0 >=  2.80, fribidi >=  1.0.6, harfbuzz >=  8.4.0, fontconfig >=  2.15.0, freetype2, cairo >=  1.18.0
Cflags: "-I${includedir}/pango-1.0"
